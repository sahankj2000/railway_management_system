<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Home.stationAlter">
   <children>
      <ImageView fitHeight="600.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Login/dark2.jpg" />
         </image>
      </ImageView>
      <Text fx:id="stAlterHeader" fill="WHITE" layoutX="227.0" layoutY="144.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Add">
         <font>
            <Font size="50.0" />
         </font>
      </Text>
      <Text fill="WHITE" layoutX="227.0" layoutY="229.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Station ID">
         <font>
            <Font size="25.0" />
         </font>
      </Text>
      <Text fill="WHITE" layoutX="227.0" layoutY="287.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Station Name">
         <font>
            <Font size="25.0" />
         </font>
      </Text>
      <TextField fx:id="stIDF" layoutX="467.0" layoutY="207.0" />
      <TextField fx:id="stNameF" layoutX="467.0" layoutY="265.0" />
      <Button fx:id="stAUButton" defaultButton="true" layoutX="467.0" layoutY="429.0" mnemonicParsing="false" onAction="#alterStation" text="Button" />
      <Button layoutX="575.0" layoutY="429.0" mnemonicParsing="false" onAction="#closeSA" text="Cancel" />
      <Text fill="WHITE" layoutX="225.0" layoutY="350.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Station Phone">
         <font>
            <Font size="25.0" />
         </font>
      </Text>
      <TextField fx:id="stTelF" layoutX="467.0" layoutY="328.0" />
   </children>
</AnchorPane>
